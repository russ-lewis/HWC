part main {
    int x = 1;
    int y = -2;

    int z;
    z = x+y;

    int a,b,c,d;
    a = x-z;
    b = a*z;
    c = b/a;
    d = c/b;

    int e,f,g;
    e = d&c;
    f = e|d;
    g = f^e;

    int h = -g;

    int i = ~h;


    bit[8] X = x;
    bit[8] Y = -y;
    bit[8] Z = -z;
    bit[8] A = a;
    bit[8] B = -b;
    bit[8] C = -c;
    bit[8] D = d;
    bit[8] E = e;
    bit[8] F = f;
    bit[8] G = g;
    bit[8] H = h;
    bit[8] I = -i;
}
